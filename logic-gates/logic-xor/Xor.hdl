/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);

    And(a=a, b=notB, out=c0);
    And(a=notA, b=b, out=c1);

    Or(a=c0, b=c1, out=out);
}